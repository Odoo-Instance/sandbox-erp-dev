<?xml version="1.0" encoding="utf-8"?>
<odoo>
        <record id="sale_order_warning_view_form" model="ir.ui.view">
            <field name="name">Invoice Orders</field>
            <field name="model">sale.order.warning</field>
            <field name="arch" type="xml">
                <form string="Invoice Sales Order">
                    <p>
                        Invoices will be created in draft so that you can review
                        them before validation.
                    </p>
                </form>
            </field>
        </record>

        <record id="action_view_sale_order_warning" model="ir.actions.act_window">
            <field name="name">Few Receipt</field>
            <field name="type">ir.actions.act_window</field>
            <field name="res_model">sale.order.warning</field>
            <field name="view_mode">form</field>
            <field name="target">new</field>
        </record>

</odoo>
